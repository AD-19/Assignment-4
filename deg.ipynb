   "source": [
    "with open('deg.txt', 'r') as file:\n",
    "    lines = file.read().splitlines()\n",
    "\n",
    "n, m = map(int, lines[0].split())\n",
    "degrees = [0] * (n + 1)\n",
    "for line in lines[1:]:\n",
    "    u, v = map(int, line.split())\n",
    "    degrees[u] += 1\n",
    "    degrees[v] += 1\n",
    "\n",
    "print(' '.join(map(str, degrees[1:])))"
   ]
  }
